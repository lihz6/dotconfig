[alias]
amend = commit --amend --reuse-message=HEAD
reword = commit --amend --message
uncommit = reset --soft HEAD~1
update = fetch --all --prune
unstage = reset HEAD --
last = log -1 HEAD
stat = diff --stat

[help]
autocorrect = 1

[user]
name = Li Hongzhang
email = lihongzhang@bytedance.com

[credential]
helper = store

[push]
default = current

[pull]
rebase = true

[core]
ignoreCase = false
