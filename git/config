[alias]
last = log -1 HEAD
amend = commit --amend --reuse-message=HEAD
update = fetch --all --prune
uncommit = reset --soft HEAD~1
unstage = reset HEAD --
stat = diff --stat

[help]
autocorrect = 1

[credential]
helper = store

[push]
default = current
[pull]
rebase = true
[core]
ignoreCase = false
